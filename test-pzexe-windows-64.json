{
	"mainClass": "zombie/gameStates/MainScreenState",
	"classpath": [
		"../bin",
		"../lib/istack-commons-runtime.jar",
		"../lib/jassimp.jar",
		"../lib/javacord-2.0.17-shaded.jar",
		"../lib/javax.activation-api.jar",
		"../lib/jaxb-api.jar",
		"../lib/jaxb-runtime.jar",
		"../lib/jinput.jar",
